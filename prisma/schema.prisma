datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model SiteAnalysis {
  id             Int      @id @default(autoincrement())
  siteUrl        String
  score          Float
  performance    Float
  accessibility  Float
  bestPractices  Float
  seo            Float
  dataAnalise    DateTime @default(now())
}
